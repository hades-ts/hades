{
    "name": "@hades-ts/docs-sidebar",
    "version": "0.0.1",
    "description": "Documentation sidebar generation library with inversify DI",
    "author": "",
    "license": "ISC",
    "homepage": "https://github.com/hades-ts/hades#readme",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/hades-ts/hades.git"
    },
    "bugs": {
        "url": "https://github.com/hades-ts/hades/issues"
    },
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsup src/index.ts --sourcemap --format cjs,esm --dts && tsc --emitDeclarationOnly --declaration",
        "rebuild": "rm -rf dist && npm run build",
        "dev": "tsup src/index.ts --watch --format cjs,esm --dts",
        "watch": "tsc -p tsconfig.json -w"
    },
    "devDependencies": {
        "@docusaurus/plugin-content-docs": "~3.8.1",
        "@types/node": "catalog:",
        "@typescript-eslint/eslint-plugin": "~6.21.0",
        "@typescript-eslint/parser": "~6.21.0",
        "eslint": "^8.57.1",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-decorator-position": "^5.0.2",
        "eslint-plugin-eslint-plugin": "^5.5.1",
        "eslint-plugin-hades": "workspace:^",
        "eslint-plugin-import": "^2.32.0",
        "eslint-plugin-import-newlines": "^1.4.0",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-simple-import-sort": "^8.0.0",
        "eslint-plugin-unused-imports": "~3.2.0",
        "ts-node": "^10.9.2",
        "tsconfig-paths": "^4.2.0",
        "tsup": "~8.0.2",
        "typescript": "~5.8.3"
    },
    "dependencies": {
        "@reliverse/pathkit": "~1.3.4",
        "gray-matter": "^4.0.3",
        "inversify": "catalog:",
        "reflect-metadata": "catalog:"
    }
}