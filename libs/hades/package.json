{
    "name": "@hades-ts/hades",
    "version": "0.0.7",
    "description": "Typescript framework using Inversify.js and Discord.js",
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsup src/index.ts --sourcemap --format cjs,esm --dts && tsc --emitDeclarationOnly --declaration",
        "rebuild": "rm -rf dist; npm run build",
        "watch": "tsc -p tsconfig.json -w"
    },
    "dependencies": {
        "common-tags": "^1.8.0",
        "config": "^3.3.3",
        "discord-api-types": "~0.33.5",
        "discord-command-parser": "^1.5.3",
        "discord.js": "14.20.0",
        "@ldlework/inversify-binding-decorators": "^4.0.2",
        "reflect-metadata": "~0.1.13",
        "@ldlework/inversify-config-injection": "~0.0.7"
    },
    "devDependencies": {
        "@types/node": "^18.11.11",
        "eslint-plugin-hades": "workspace:^0.0.1",
        "eslint": "^8.26.0",
        "eslint-plugin-eslint-plugin": "^5.0.0",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-decorator-position": "^5.0.1",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-import-newlines": "^1.2.3",
        "eslint-plugin-simple-import-sort": "^8.0.0",
        "eslint-plugin-unused-imports": "3.x",
        "@typescript-eslint/eslint-plugin": "6.x",
        "@typescript-eslint/parser": "6.x",
        "commander": "^7.2.0",
        "ts-node": "^10.9.1",
        "tsconfig-paths": "^4.1.1",
        "typescript": "~5.8.3",
        "inversify": "^6.0.1",
        "eslint-config-prettier": "^9.0.0",
        "tsup": "~8.0.1"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/hades-ts/hades.git"
    },
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/hades-ts/hades/issues"
    },
    "homepage": "https://github.com/hades-ts/hades#readme"
}
