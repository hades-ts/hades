{
    "name": "@hades-ts/tsx",
    "description": "Create Discord.js components in JSX",
    "version": "0.0.7",
    "type": "module",
    "author": "hades-ts",
    "license": "MIT-0",
    "homepage": "https://github.com/hades-ts/hades#readme",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/hades-ts/hades.git"
    },
    "bugs": {
        "url": "https://github.com/hades-ts/hades/issues"
    },
    "keywords": [
        "jsx",
        "tsx",
        "discord"
    ],
    "types": "./dist/index.d.ts",
    "exports": {
        "import": "./dist/index.js",
        "require": "./dist/index.cjs"
    },
    "packageManager": "pnpm@10.12.1",
    "scripts": {
        "build": "tsup",
        "lint": "tsc --noEmit",
        "test:babel": "node test/js-out/babel.js",
        "test:babel:build": "babel test/js/babel.jsx --relative -d ../js-out",
        "test:ts": "tsx --tsconfig test/tsconfig.json test/ts/typescript.tsx"
    },
    "files": [
        "dist"
    ],
    "devDependencies": {
        "@babel/cli": "^7.27.2",
        "@babel/core": "^7.27.4",
        "@babel/plugin-transform-react-jsx": "^7.27.1",
        "@babel/preset-env": "^7.27.2",
        "@types/node": "^24.0.3",
        "discord.js": "catalog:",
        "tsup": "catalog:",
        "tsx": "^4.20.3",
        "turbo": "catalog:",
        "typescript": "catalog:"
    }
}