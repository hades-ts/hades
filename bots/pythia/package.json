{
    "private": true,
    "name": "@hades-ts/pythia",
    "version": "0.0.0",
    "description": "Hades based GPT3 Discord bot",
    "type": "commonjs",
    "main": "./dist/index.js",
    "scripts": {
        "build": "pkgroll --sourcemap && tsc --emitDeclarationOnly --declaration --declarationMap",
        "clean": "rimraf dist",
        "lint": "tsc --noEmit",
        "start": "node dist/index.js",
        "watch": "tsc -p tsconfig.json -w",
        "db:rm": "docker rm pythia-db",
        "db:start": "docker run --name pythia-db -e POSTGRES_USER=postgres -e POSTGRES_PASSWORD=postgres -e POSTGRES_DB=postgres -d -p 5432:5432 pgvector/pgvector:pg17",
        "db:stop": "docker stop pythia-db",
        "db:push": "drizzle-kit push",
        "db:generate": "drizzle-kit generate",
        "db:migrate": "drizzle-kit migrate",
        "db:studio": "drizzle-kit studio",
        "db:check": "drizzle-kit check"
    },
    "dependencies": {
        "@ai-sdk/openai": "^1.3.22",
        "@hades-ts/bypass": "workspace:^",
        "@hades-ts/core": "workspace:^",
        "@hades-ts/guilds": "workspace:^",
        "@hades-ts/slash-commands": "workspace:^",
        "@hades-ts/stash": "workspace:^",
        "@ldlework/categoric-containers": "catalog:",
        "@ldlework/categoric-decorators": "catalog:",
        "@ldlework/inversify-binding-decorators": "catalog:",
        "ai": "^4.3.16",
        "axios": "^1.10.0",
        "common-tags": "^1.8.2",
        "config": "^3.3.12",
        "discord-command-parser": "^1.5.3",
        "discord.js": "catalog:",
        "dotenv": "^16.6.0",
        "drizzle-orm": "^0.44.2",
        "drizzle-zod": "^0.8.2",
        "googleapis": "^67.1.1",
        "gpt3-tokenizer": "^1.1.5",
        "inversify": "catalog:",
        "nanoid": "^5.1.5",
        "node-schedule": "^1.3.3",
        "pg": "^8.16.3",
        "postgres": "^3.4.7",
        "reflect-metadata": "catalog:",
        "rxjs": "^7.8.2",
        "sqlite3": "^5.1.7",
        "typeorm": "~0.2.45",
        "zod": "~3.25.67"
    },
    "devDependencies": {
        "@types/common-tags": "^1.8.4",
        "@types/node": "catalog:",
        "@types/node-schedule": "^1.3.2",
        "@types/pg": "^8.15.4",
        "@types/ws": "^8.18.1",
        "@typescript-eslint/eslint-plugin": "~6.21.0",
        "@typescript-eslint/parser": "~6.21.0",
        "drizzle-kit": "^0.31.4",
        "eslint": "^8.57.1",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-decorator-position": "^5.0.2",
        "eslint-plugin-eslint-plugin": "^5.5.1",
        "eslint-plugin-import": "^2.32.0",
        "eslint-plugin-import-newlines": "^1.4.0",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-simple-import-sort": "^8.0.0",
        "eslint-plugin-unused-imports": "~3.2.0",
        "pkgroll": "catalog:",
        "rimraf": "catalog:",
        "tsconfig-paths": "^4.2.0",
        "tsx": "catalog:",
        "typescript": "catalog:"
    }
}
